<div id="topbar">
    <section id="page-title">
        <h4>@ViewBag.Title</h4>
    </section>
</div>

<div id="form-area">
    <form id="form" autocomplete="off">
        <div class="action-row">
            <div class="row">
                <input class="button button-primary u-pull-right" type="submit" value="Save">
                <input class="button u-pull-right" type="button" value="Cancel">
            </div>
        </div>
    </form>
</div>